# Environment Variables Template
# Copy this file to .env and fill in your actual values

# WooCommerce Store URL (no trailing slash)
VITE_STORE_URL=https://umkmbsi.com

# WooCommerce REST API Credentials
# Generate from: WordPress Admin -> WooCommerce -> Settings -> Advanced -> REST API
VITE_WC_CONSUMER_KEY=ck_your_consumer_key_here
VITE_WC_CONSUMER_SECRET=cs_your_consumer_secret_here

# API Versions (optional - defaults provided)
VITE_WC_API_VERSION=v3
VITE_WC_STORE_API_VERSION=v1

# Development Settings
VITE_DEV_MODE=true
VITE_API_TIMEOUT=30000
